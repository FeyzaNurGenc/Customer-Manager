    * Author: BootstrapMade.com
    * License: https://bootstrapmade.com/license/
    ======================================================== -->
</head>

<body class="index-page">

    <header id="header" class="header d-flex flex-column justify-content-center">

        <i class="header-toggle d-xl-none bi bi-list"></i>

        <nav id="navmenu" class="navmenu">
            <ul>
                <li><a href="/Admin/Index"><i class="bi bi-speedometer2"></i><span>Panel</span></a></li>
                <li><a href="#hero" class="active"><i class="bi bi-house navicon"></i><span>Home</span></a></li>
                <li><a href="#about"><i class="bi bi-person navicon"></i><span>About</span></a></li>
                <li><a href="#resume"><i class="bi bi-file-earmark-text navicon"></i><span>Resume</span></a></li>
                <li><a href="#portfolio"><i class="bi bi-images navicon"></i><span>Portfolio</span></a></li>
                <li><a href="#services"><i class="bi bi-hdd-stack navicon"></i><span>Services</span></a></li>
                <li><a href="#contact"><i class="bi bi-envelope navicon"></i><span>Contact</span></a></li>
            </ul>
        </nav>

    </header>

    <main class="main">

        <!-- Hero Section -->
        <section id="hero" class="hero section light-background">

            @foreach (var i in Model.About)
            {
                <img src="@i.PhotoPath" alt="Fotoğraf" style="height: auto;" data-aos="fade-in" />
            }

            <div class="container" data-aos="zoom-out">
                <div class="row justify-content-center">
                    <div class="col-lg-9">
                        <h2>Merhaba ben @Model.Customer.CustomerKey </h2>
                        <p><span class="typed" data-typed-items="Designer, Developer, Freelancer, Photographer">Designer</span><span class="typed-cursor typed-cursor--blink" aria-hidden="true"></span></p>
                      
                    </div>
                </div>
            </div>

        </section><!-- /Hero Section -->
        <!-- About Section -->
        <section id="about" class="about section">

            <!-- Section Title -->
            <div class="container section-title" data-aos="fade-up">
                <h2>Hakkında</h2>
              
            </div><!-- End Section Title -->

            <div class="container" data-aos="fade-up" data-aos-delay="100">

                <div class="row gy-4 justify-content-center">
                    <div class="col-lg-4">
                        @foreach (var i in Model.About)
                        {
                            <img src="@i.PhotoPath" alt="Fotoğraf" style="height:200px;width:200px;" data-aos="fade-in" />
                        }
                    </div>
                    <div class="col-lg-8 content">
                     
                        
                        <div class="row">
                          
                            <div class="col-lg-6">
                                <ul>
                                    <li><i class="bi bi-chevron-right"></i> <strong>İsim:</strong> <span>@Model.CustomerKey</span></li>
                                    <li><i class="bi bi-chevron-right"></i> <strong>Telefon:</strong> <span>@Model.Contact.FirstOrDefault()?.Phone</span></li>
                                    <li><i class="bi bi-chevron-right"></i> <strong>Email:</strong> <span>@Model.Customer.Email</span></li>
                                   
                                </ul>
                            </div>
                            <div class="col-md-6">
                                <div class="about-me">
                                    <h4>Hakkımda</h4>
                                    @foreach (var i in Model.About)
                                    {
                                        <p>
                                            @i.Description
                                        </p>
                                    }

                                </div>
                            </div>
                        </div>
                    </div>
                    
                </div>

            </div>

        </section><!-- /About Section -->
     
        <!-- Skills Section -->
        <section id="skills" class="skills section">

            <!-- Section Title -->
            <div class="container section-title" data-aos="fade-up">
                <h2>Yetenekler</h2>
                
            </div><!-- End Section Title -->

            <div class="container" data-aos="fade-up" data-aos-delay="100">

                <div class="row skills-content skills-animation">

                    <div class="col-lg-6">
                        @foreach(var i in Model.Skills){
                            <div class="progress">
                                <span class="skill"><span>@i.Title</span> <i class="val">@i.Value%</i></span>
                            <div class="progress-bar-wrap">
                                    <div class="progress-bar" role="progressbar" aria-valuenow="@i.Value" aria-valuemin="0" aria-valuemax="100"></div>
                            </div>
                        </div><!-- End Skills Item -->}
   
                    </div>

                </div>

            </div>

        </section><!-- /Skills Section -->
        <!-- Resume Section -->
        <section id="resume" class="resume section">

            <!-- Section Title -->
            <div class="container section-title" data-aos="fade-up">
                <h2>Özgeçmiş</h2>
              
            </div><!-- End Section Title -->

            <div class="container">
                <div class="row" data-aos="fade-up" data-aos-delay="100">
                    @foreach (var i in Model.Ozgecmis)
                    {
                        <div class="col-lg-4">
                            <h3 class="resume-title">@i.Title</h3>
                            <div class="resume-item">
                                <h4>@i.Title1</h4>
                                <h5>@i.YearStart / @i.YearFinish</h5>
                                <p>@i.Description</p>
                            </div><!-- End Resume Item -->
                        </div>
                    }
                </div>
            </div>


        </section><!-- /Resume Section -->
        <!-- Portfolio Section -->
        <section id="portfolio" class="portfolio section">

            <!-- Section Title -->
            <div class="container section-title" data-aos="fade-up">
                <h2>Projeler</h2>
               
            </div><!-- End Section Title -->

            <div class="container">

                <div class="isotope-layout" data-default-filter="*" data-layout="masonry" data-sort="original-order">

                    <ul class="portfolio-filters isotope-filters" data-aos="fade-up" data-aos-delay="100">
                        @foreach(var i in Model.Portfolyo){
                             <li data-filter="*" class="filter-active">@i.Category</li>
                        }
                       
                        
                    </ul><!-- End Portfolio Filters -->

                    <div class="row gy-4 isotope-container" data-aos="fade-up" data-aos-delay="200">
                        @foreach(var i in Model.Portfolyo){
                             <div class="col-lg-4 col-md-6 portfolio-item isotope-item filter-app">
                            <img src="@i.PhotoPath" class="img-fluid" alt="">
                            <div class="portfolio-info">
                                <h4>@i.ProjectName</h4>
                                <p>@i.Descripton</p>
                                <a href="assets/img/masonry-portfolio/masonry-portfolio-1.jpg" title="App 1" data-gallery="portfolio-gallery-app" class="glightbox preview-link"><i class="bi bi-zoom-in"></i></a>
                                <a href="portfolio-details.html" title="More Details" class="details-link"><i class="bi bi-link-45deg"></i></a>
                            </div>
                        </div><!-- End Portfolio Item -->
                        }

                    </div><!-- End Portfolio Container -->

                </div>

            </div>

        </section><!-- /Portfolio Section -->
        <!-- Services Section -->
        <section id="services" class="services section">

            <!-- Section Title -->
            <div class="container section-title" data-aos="fade-up">
                <h2>Hizmetler</h2>
               
            </div><!-- End Section Title -->

            <div class="container">

                <div class="row gy-4">
                    @foreach(var i in Model.Hizmetler){
                             <div class="col-lg-4 col-md-6" data-aos="fade-up" data-aos-delay="100">
                        <div class="service-item item-cyan position-relative">
                            <div class="icon">
                                <svg width="100" height="100" viewBox="0 0 600 600" xmlns="http://www.w3.org/2000/svg">
                                    <path stroke="none" stroke-width="0" fill="#f5f5f5" d="M300,521.0016835830174C376.1290562159157,517.8887921683347,466.0731472004068,529.7835943286574,510.70327084640275,468.03025145048787C554.3714126377745,407.6079735673963,508.03601936045806,328.9844924480964,491.2728898941984,256.3432110539036C474.5976632858925,184.082847569629,479.9380746630129,96.60480741107993,416.23090153303,58.64404602377083C348.86323505073057,18.502131276798302,261.93793281208167,40.57373210992963,193.5410806939664,78.93577620505333C130.42746243093433,114.334589627462,98.30271207620316,179.96522072025542,76.75703585869454,249.04625023123273C51.97151888228291,328.5150500222984,13.704378332031375,421.85034740162234,66.52175969318436,486.19268352777647C119.04800174914682,550.1803526380478,217.28368757567262,524.383925680826,300,521.0016835830174"></path>
                                </svg>
                                <i class="@i.Icon"></i>
                            </div>
                            <a href="#" class="stretched-link">
                                <h3>@i.Title</h3>
                            </a>
                            <p>@i.Description</p>
                        </div>
                    </div><!-- End Service Item -->
 
                    }

                </div>

            </div>

        </section><!-- /Services Section -->
        <!-- Faq Section -->
        <!-- Section Title -->
        <div class="container section-title" data-aos="fade-up">
            <h2>Soru-Cevap</h2>

        </div><!-- End Section Title -->
        <section id="faq" class="faq section">

            <div class="container">

              

                    <div class="col-lg-8" data-aos="fade-up" data-aos-delay="200">
                   
                        <div class="faq-container">

                            @{
                                
                                int sayac = 1;
                            }

                            @foreach (var i in Model.SSS)
                            {
                                <div class="faq-item faq-active">
                                    <h3><span class="num">@sayac.</span> <span>@i.Soru</span></h3>
                                    <div class="faq-content">
                                        <p>@i.Cevap</p>
                                    </div>
                                    
                                </div>
                                sayac++;
                                <!-- End Faq item-->
                            }



                        </div>

                    </div>
                

            </div>

        </section><!-- /Faq Section -->



        <!-- Contact Section -->
        <section id="contact" class="contact section">

            <!-- Section Title -->
            <div class="container section-title" data-aos="fade-up">
                <h2>İletişim</h2>
             
            </div><!-- End Section Title -->

            <div class="container" data-aos="fade" data-aos-delay="100">

                <div class="row gy-4">
                    @foreach (var i in Model.Contact)
                    {
                    <div class="col-lg-4">
                       
                            <div class="info-item d-flex" data-aos="fade-up" data-aos-delay="200">
                            <i class="bi bi-geo-alt flex-shrink-0"></i>
                            <div>
                                <h3>Adres</h3>
                                <p>@i.Address</p>
                            </div>
                           
                            </div>
                       
                    </div>

                    <div class="col-lg-4">
                            <div class="info-item d-flex" data-aos="fade-up" data-aos-delay="200">
                                <i class="bi bi-telephone flex-shrink-0"></i>
                                <div>
                                    <h3>Telefon</h3>
                                    <p>@i.Phone</p>
                                </div>

                            </div>
                    </div>
                        <div class="col-lg-4">
                            <div class="info-item d-flex" data-aos="fade-up" data-aos-delay="200">
                                <i class="bi bi-envelope flex-shrink-0"></i>
                                <div>
                                    <h3>Mail</h3>
                                    <p>@Model.Customer.Email</p>
                                </div>

                            </div>
                        </div>
                    }

                </div>

            </div>

        </section><!-- /Contact Section -->

    </main>

    <footer id="footer" class="footer position-relative light-background">
        <div class="container">
           
                <h3 class="sitename">@Model.Customer.CustomerKey</h3>
          
            <div class="container">
                <div class="copyright">
                    <span>Copyright</span> <strong class="px-1 sitename">Alex Smith</strong> <span>All Rights Reserved</span>
                </div>
                <div class="credits">
                    <!-- All the links in the footer should remain intact. -->
                    <!-- You can delete the links only if you've purchased the pro version. -->
                    <!-- Licensing information: https://bootstrapmade.com/license/ -->
                    <!-- Purchase the pro version with working PHP/AJAX contact form: [buy-url] -->
                    Designed by <a href="https://bootstrapmade.com/">BootstrapMade</a>
                </div>
            </div>
        </div>
    </footer>

    <!-- Scroll Top -->
    <a href="#" id="scroll-top" class="scroll-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Preloader -->
    <div id="preloader"></div>

    <!-- Vendor JS Files -->
    <script src="~/MyResume/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/MyResume/assets/vendor/php-email-form/validate.js"></script>
    <script src="~/MyResume/assets/vendor/aos/aos.js"></script>
    <script src="~/MyResume/assets/vendor/typed.js/typed.umd.js"></script>
    <script src="~/MyResume/assets/vendor/purecounter/purecounter_vanilla.js"></script>
    <script src="~/MyResume/assets/vendor/waypoints/noframework.waypoints.js"></script>
    <script src="~/MyResume/assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/MyResume/assets/vendor/imagesloaded/imagesloaded.pkgd.min.js"></script>
    <script src="~/MyResume/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/MyResume/assets/vendor/swiper/swiper-bundle.min.js"></script>

    <!-- Main JS File -->
    <script src="~/MyResume/assets/js/main.js"></script>

</body>

</html>